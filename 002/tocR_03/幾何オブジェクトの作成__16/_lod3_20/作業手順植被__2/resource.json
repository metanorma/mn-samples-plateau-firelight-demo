    ],
    [
        "urn:x-xml-element:li-1c5a9de3-e3c1-4835-860a-7159d7280b04",
        "hasPart",
        "\n"
    ],
    [
        "urn:x-metanorma-xml-id:_8d3b4e8a-e11b-55e4-14a5-a03a5b640ac2",
        "type",
        "paragraph"
    ],
    [
        "urn:x-metanorma-xml-id:_8d3b4e8a-e11b-55e4-14a5-a03a5b640ac2",
