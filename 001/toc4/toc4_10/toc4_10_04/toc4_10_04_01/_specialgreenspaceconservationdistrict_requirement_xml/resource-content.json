                                    "content": [
                                        {
                                            "type": "table_cell",
                                            "attrs": {
                                                "colspan": null,
                                                "rowspan": null,
                                                "colwidth": null,
                                                "background": null
                                            },
                                            "content": [
                                                {
                                                    "type": "paragraph",
                                                    "attrs": {
                                                        "resourceID": "urn:x-metanorma-xml-id:_c60d83c5-4aca-44aa-3c65-9c15161d19a3"
                                                    },
                                                    "content": [
                                                        {
                                                            "type": "text",
                                                            "text": "2"
                                                        }
                                                    ]
                                                }
                                            ]
                                        },
                                        {
                                            "type": "table_cell",
                                            "attrs": {
                                                "colspan": null,
                                                "rowspan": null,
                                                "colwidth": null,
                                                "background": null
                                            },
                                            "content": [
