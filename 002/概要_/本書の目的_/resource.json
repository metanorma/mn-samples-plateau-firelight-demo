    ],
    [
        "urn:x-xml-element:link-ac359463-1973-4559-a6d7-0e7a406463fd",
        "hasPart",
        "3D都市モデル整備のための測量マニュアル"
    ],
    [
        "urn:x-metanorma-xml-id:_5fdc2ee7-b315-079f-665a-a969fc9189fa",
