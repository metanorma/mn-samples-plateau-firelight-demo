    [
        "urn:x-xml-element:link-0ac05c42-90b4-4756-863d-507f89df71e4",
        "hasPart",
        "PLATEAU Handbook #03"
    ],
    [
        "urn:x-metanorma-xml-id:_6fcc18ca-b556-7504-26b5-ab2f70ca2230",
